{
  "name": "letteweb",
  "description": "Iteration of Letteweb built with Gatsby",
  "version": "1.0.0",
  "author": "Me",
  "keywords": [
    "gatsby",
    "tailwind",
    "tailwindcss",
    "purgecss"
  ],
  "dependencies": {
    "gatsby": "2.24.3",
    "gatsby-background-image": "^1.1.1",
    "gatsby-image": "^2.4.13",
    "gatsby-plugin-manifest": "2.4.18",
    "gatsby-plugin-offline": "3.2.18",
    "gatsby-plugin-postcss": "2.3.11",
    "gatsby-plugin-purgecss": "5.0.0",
    "gatsby-plugin-react-helmet": "3.3.10",
    "gatsby-plugin-sharp": "2.6.19",
    "gatsby-plugin-sitemap": "^2.4.11",
    "gatsby-plugin-styled-jsx": "^3.3.11",
    "gatsby-source-filesystem": "^2.3.19",
    "gatsby-transformer-sharp": "^2.5.11",
    "i18next": "^19.6.2",
    "i18next-browser-languagedetector": "^5.0.0",
    "i18next-http-backend": "^1.0.17",
    "intersection-observer": "^0.11.0",
    "prop-types": "15.7.2",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-helmet": "6.1.0",
    "react-i18next": "^11.7.0",
    "react-intersection-observer": "^8.26.2",
    "react-particles-js": "^3.2.1",
    "styled-jsx": "^3.3.0"
  },
  "license": "MIT",
  "scripts": {
    "analyze:lint": "eslint --ext .jsx --ext .js .",
    "analyze:prettier": "prettier --list-different \"**/*.{css,js,jsx,json,md}\"",
    "analyze": "npm run analyze:lint && npm run analyze:prettier",
    "fix:lint": "eslint --ext .jsx --ext .js . --fix",
    "fix:prettier": "prettier --write \"**/*.{css,js,jsx,json,md}\"",
    "fix": "npm run fix:lint && npm run fix:prettier",
    "build": "gatsby build",
    "develop": "gatsby develop -O",
    "develop2": "gatsby develop -H 0.0.0.0",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1"
  },
  "devDependencies": {
    "autoprefixer": "9.8.5",
    "babel-eslint": "10.1.0",
    "cssnano": "4.1.10",
    "eslint": "7.4.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-loader": "4.0.2",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "7.20.3",
    "eslint-plugin-react-hooks": "^4.0.8",
    "gatsby-plugin-eslint": "2.0.8",
    "prettier": "2.0.5",
    "stylelint-config-recommended": "^3.0.0",
    "tailwindcss": "1.5.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/taylorbryant/gatsby-starter-tailwind"
  },
  "bugs": {
    "url": "https://github.com/taylorbryant/gatsby-starter-tailwind/issues"
  }
}
