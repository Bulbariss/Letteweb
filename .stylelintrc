{
  "extends": "stylelint-config-recommended",
  "rules": {
    "at-rule-no-unknown": [ true, {
      "ignoreAtRules": [
        "extends",
        "tailwind",
        "screen"
      ]
    }],
    "block-no-empty": null,
    "no-descending-specificity": null,
    "media-feature-name-no-unknown": null,
    "unit-whitelist": ["em", "rem", "s","px", "%", "vh", "vw","ms"]
  }
}